#include "lists.h"

/**
 *add_nodeint - function that adds a new node at the beginning
 *@head: head of listint
 *@n: to be checked
 *Return: adress of new element
 */
listint_t *add_nodeint(listint_t **head, const int n)
{
listint_t *next;
new = malloc(sizeof(listint_t));
if (new == NULL)
return (NULL);
new->n = n;
new->next = *head;
*head = new;
return (new);
}
